    <!-- http://purl.org/net/CCL#Chinese_Belief_1 -->

    <owl:NamedIndividual rdf:about="http://purl.org/net/CCL#Chinese_Belief_1">
        <rdf:type rdf:resource="http://purl.org/net/CCL#CulturalBelief"/>
        <hasAssociatedCulturalValue rdf:resource="http://purl.org/net/CCL#Chinese_Value_1"/>
