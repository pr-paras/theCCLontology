    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/net/CCL#categoryInvokesSchema -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#categoryInvokesSchema">
        <owl:inverseOf rdf:resource="http://purl.org/net/CCL#isASchemaUnderlying"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/net/CCL#Category"/>
                    <rdf:Description rdf:about="http://purl.org/net/CCL#Metaphor"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://purl.org/net/CCL#Schema"/>
        <rdfs:label xml:lang="en">Invokes Schema</rdfs:label>
        <skos:definition xml:lang="en">The schema that a category invokes</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#containsEvent -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#containsEvent">
        <owl:inverseOf rdf:resource="http://purl.org/net/CCL#isAnEventContainedIn"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/net/CCL#SocialPractice"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
        <rdfs:label xml:lang="en">Contains Event</rdfs:label>
        <skos:definition xml:lang="en">Contains Some Event</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#containsInformationAbout -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#containsInformationAbout">
        <rdfs:domain rdf:resource="http://purl.org/net/CCL#Context"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Abstract"/>
                    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
                    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject"/>
                    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationRealization"/>
                    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object"/>
                    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Quality"/>
                    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label xml:lang="en">Contains Information About</rdfs:label>
        <skos:definition xml:lang="en">Contains Information About</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#containsMetonym -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#containsMetonym">
        <owl:inverseOf rdf:resource="http://purl.org/net/CCL#isAMetonymUsedIn"/>
        <rdfs:domain rdf:resource="http://purl.org/net/CCL#Metaphor"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
        <rdfs:label xml:lang="en">Contains Metonym</rdfs:label>
        <skos:definition xml:lang="en">Metonym contained in the metaphor.Metonymy is the practice of using a concept which is a part of or closely associated with another to reference it.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#containsSubmetaphor -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#containsSubmetaphor">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/CCL#hasAssociatedMetaphor"/>
        <owl:inverseOf rdf:resource="http://purl.org/net/CCL#isASubmetaphorOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/net/CCL#Metaphor"/>
        <rdfs:range rdf:resource="http://purl.org/net/CCL#Metaphor"/>
        <rdfs:label xml:lang="en">Contains Submetaphor</rdfs:label>
        <skos:definition xml:lang="en">Other metaphors contained within the this metaphor.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#encodesBehaviour -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#encodesBehaviour">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://purl.org/net/CCL#isABehaviourEncodedIn"/>
        <rdfs:domain rdf:resource="http://purl.org/net/CCL#Schema"/>
        <rdfs:range rdf:resource="http://purl.org/net/CCL#Behaviour"/>
        <rdfs:label xml:lang="en">Encodes Behaviour</rdfs:label>
        <skos:definition xml:lang="en">Encoded Behaviour within some schema</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#encodesCategory -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#encodesCategory">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/CCL#encodesCollectiveCognitionStructure"/>
        <owl:inverseOf rdf:resource="http://purl.org/net/CCL#isACategoryEncodedIn"/>
        <rdfs:domain rdf:resource="http://purl.org/net/CCL#Schema"/>
        <rdfs:range rdf:resource="http://purl.org/net/CCL#Category"/>
        <rdfs:label xml:lang="en">Encodes Category</rdfs:label>
        <skos:definition xml:lang="en">One or more categories encoded in the schema</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#encodesCollectiveCognitionStructure -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#encodesCollectiveCognitionStructure">
        <owl:inverseOf rdf:resource="http://purl.org/net/CCL#isACollectiveCognitionStructureEncodedIn"/>
        <rdfs:domain rdf:resource="http://purl.org/net/CCL#Schema"/>
        <rdfs:range rdf:resource="http://purl.org/net/CCL#CollectiveCognitionStructure"/>
        <rdfs:label>Encodes Collective Cognition Structure</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#encodesContext -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#encodesContext">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://purl.org/net/CCL#isAContextEncodedIn"/>
        <rdfs:domain rdf:resource="http://purl.org/net/CCL#Schema"/>
        <rdfs:range rdf:resource="http://purl.org/net/CCL#Context"/>
        <rdfs:label xml:lang="en">Encodes Context</rdfs:label>
        <skos:definition xml:lang="en">Encoded Context within some schema</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#encodesCulturalAgreementStructure -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#encodesCulturalAgreementStructure">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://purl.org/net/CCL#isACulturalStructureEncodedIn"/>
        <rdfs:domain rdf:resource="http://purl.org/net/CCL#Schema"/>
        <rdfs:range rdf:resource="http://purl.org/net/CCL#CulturalAgreementStructure"/>
        <rdfs:label xml:lang="en">Encodes Cultural Agreement Structure</rdfs:label>
        <skos:definition xml:lang="en">Some cultural belief/norm/value encoded within a schema</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#encodesCulturalBelief -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#encodesCulturalBelief">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/CCL#encodesCulturalAgreementStructure"/>
        <owl:inverseOf rdf:resource="http://purl.org/net/CCL#isACulturalBeliefEncodedIn"/>
        <rdfs:domain rdf:resource="http://purl.org/net/CCL#Schema"/>
        <rdfs:range rdf:resource="http://purl.org/net/CCL#CulturalBelief"/>
        <rdfs:label xml:lang="en">Encodes Cultural Belief</rdfs:label>
        <skos:definition xml:lang="en">Some cultural belief encoded within a schema</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#encodesCulturalNorm -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#encodesCulturalNorm">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/CCL#encodesCulturalAgreementStructure"/>
        <owl:inverseOf rdf:resource="http://purl.org/net/CCL#isACulturalNormEncodedIn"/>
        <rdfs:domain rdf:resource="http://purl.org/net/CCL#Schema"/>
        <rdfs:range rdf:resource="http://purl.org/net/CCL#CulturalNorm"/>
        <rdfs:label xml:lang="en">Encodes Cultural Norm</rdfs:label>
        <skos:definition xml:lang="en">Some cultural norm encoded within a schema</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#encodesCulturalValue -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#encodesCulturalValue">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/CCL#encodesCulturalAgreementStructure"/>
        <owl:inverseOf rdf:resource="http://purl.org/net/CCL#isACulturalValueEncodedIn"/>
        <rdfs:domain rdf:resource="http://purl.org/net/CCL#Schema"/>
        <rdfs:range rdf:resource="http://purl.org/net/CCL#CulturalValue"/>
        <rdfs:label xml:lang="en">Encodes Cultural Value</rdfs:label>
        <skos:definition xml:lang="en">Some cultural value encoded within a schema</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#encodesMetaphor -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#encodesMetaphor">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/CCL#encodesCollectiveCognitionStructure"/>
        <owl:inverseOf rdf:resource="http://purl.org/net/CCL#isAMetaphorEncodedIn"/>
        <rdfs:domain rdf:resource="http://purl.org/net/CCL#Schema"/>
        <rdfs:range rdf:resource="http://purl.org/net/CCL#Metaphor"/>
        <rdfs:label xml:lang="en">Encodes Metaphor</rdfs:label>
        <skos:definition xml:lang="en">One or more metaphors encoded in the schema</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#encodesRule -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#encodesRule">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://purl.org/net/CCL#isARuleEncodedIn"/>
        <rdfs:domain rdf:resource="http://purl.org/net/CCL#Schema"/>
        <rdfs:range rdf:resource="http://purl.org/net/CCL#Rule"/>
        <rdfs:label xml:lang="en">Encodes Rule</rdfs:label>
        <skos:definition xml:lang="en">Encoded Rule within some schema</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#encodesSchema -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#encodesSchema">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/CCL#encodesCollectiveCognitionStructure"/>
        <owl:inverseOf rdf:resource="http://purl.org/net/CCL#isASchemaEncodedIn"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/net/CCL#ComplexSchema"/>
        <rdfs:range rdf:resource="http://purl.org/net/CCL#Schema"/>
        <rdfs:label xml:lang="en">Encodes Schema</rdfs:label>
        <skos:definition xml:lang="en">Sub-schemas encoded in the schema</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#encodesSocialPractice -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#encodesSocialPractice">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://purl.org/net/CCL#isASocialPracticeEncodedIn"/>
        <rdfs:domain rdf:resource="http://purl.org/net/CCL#ComplexSchema"/>
        <rdfs:range rdf:resource="http://purl.org/net/CCL#SocialPractice"/>
        <rdfs:label xml:lang="en">Encodes Social Practice</rdfs:label>
        <skos:definition xml:lang="en">Encoded Social Practice within some schema</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#hasAlignmentRelationshipWith -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#hasAlignmentRelationshipWith">
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Goal"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/net/CCL#CulturalAgreementStructure"/>
                    <rdf:Description rdf:about="http://purl.org/net/CCL#Rule"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label xml:lang="en">Has Alignment Relationship With</rdfs:label>
        <skos:definition xml:lang="en">Assigns alignment objective of a  concept with another which it wants to attain alignment objective - Rule/Norm/Value/Belief</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#hasAssociatedAction -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#hasAssociatedAction">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/CCL#hasAssociatedEvent"/>
        <owl:inverseOf rdf:resource="http://purl.org/net/CCL#isAnActionAssociatedWith"/>
        <rdfs:domain rdf:resource="http://purl.org/net/CCL#Behaviour"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Action"/>
        <rdfs:label xml:lang="en">Has Associated Action</rdfs:label>
        <skos:definition xml:lang="en">One or more associated actions</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#hasAssociatedArtefact -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#hasAssociatedArtefact">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/CCL#hasAssociatedSocialObject"/>
        <owl:inverseOf rdf:resource="http://purl.org/net/CCL#isAnAssociatedArtefactOf"/>
        <rdfs:range rdf:resource="http://purl.org/net/CCL#CulturalArtefact"/>
        <rdfs:label xml:lang="en">Has Associated Artefact</rdfs:label>
        <skos:definition xml:lang="en">Some Associated Artefact</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#hasAssociatedAtomicCategory -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#hasAssociatedAtomicCategory">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/CCL#hasAssociatedCategory"/>
        <owl:inverseOf rdf:resource="http://purl.org/net/CCL#isAnAssociatedAtomicCategoryOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/net/CCL#CategoryMembershipRule"/>
                    <rdf:Description rdf:about="http://purl.org/net/CCL#CollectiveCognitionStructure"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://purl.org/net/CCL#CognitiveCategory"/>
        <rdfs:label xml:lang="en">Has Associated Atomic Category</rdfs:label>
        <skos:definition xml:lang="en">Assigns associated atomic categories</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#hasAssociatedAtomicMetaphor -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#hasAssociatedAtomicMetaphor">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/CCL#hasAssociatedMetaphor"/>
        <owl:inverseOf rdf:resource="http://purl.org/net/CCL#isAnAssociatedAtomicMetaphorOf"/>
        <rdfs:domain rdf:resource="http://purl.org/net/CCL#Metaphor"/>
        <rdfs:range rdf:resource="http://purl.org/net/CCL#AtomicMetaphor"/>
        <rdfs:label xml:lang="en">Has Associated Atomic Metaphor</rdfs:label>
        <skos:definition xml:lang="en">Assigns an associated atomic metaphor</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#hasAssociatedAtomicSchema -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#hasAssociatedAtomicSchema">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/CCL#hasAssociatedSchema"/>
        <owl:inverseOf rdf:resource="http://purl.org/net/CCL#isAnAssociatedAtomicSchemaOf"/>
        <rdfs:domain rdf:resource="http://purl.org/net/CCL#CollectiveCognitionStructure"/>
        <rdfs:range rdf:resource="http://purl.org/net/CCL#AtomicSchema"/>
        <rdfs:label xml:lang="en">Has Associated Atomic Schema</rdfs:label>
        <skos:definition xml:lang="en">Assigns associated atomic schemas</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#hasAssociatedBehaviour -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#hasAssociatedBehaviour">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/CCL#hasAssociatedAction"/>
        <owl:inverseOf rdf:resource="http://purl.org/net/CCL#isABehaviourAssociatedWith"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/net/CCL#CulturalNorm"/>
                    <rdf:Description rdf:about="http://purl.org/net/CCL#SocialPractice"/>
                    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Goal"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://purl.org/net/CCL#Behaviour"/>
        <rdfs:label xml:lang="en">Has Associated Behaviour</rdfs:label>
        <skos:definition xml:lang="en">One or more associated behaviours especially for rule and behaviour schemas</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/CCL#hasAssociatedBelief -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/CCL#hasAssociatedBelief">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/CCL#hasAssociatedCulturalAgreementStructure"/>
        <owl:inverseOf rdf:resource="http://purl.org/net/CCL#isACulturalBeliefAssociatedWith"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/net/CCL#CulturalNorm"/>
                    <rdf:Description rdf:about="http://purl.org/net/CCL#CulturalValue"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://purl.org/net/CCL#CulturalBelief"/>
        <rdfs:label xml:lang="en">Has Associated Cultural Belief</rdfs:label>
        <skos:definition xml:lang="en">One or more associated cultural beliefs</skos:definition>
