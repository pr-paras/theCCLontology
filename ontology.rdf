    <!-- http://purl.org/dc/terms/LinguisticSystem -->


    <owl:Class rdf:about="http://purl.org/dc/terms/LinguisticSystem">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#SocioCulturalStructure"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasDescription"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Linguistic System</rdfs:label>
        <skos:definition xml:lang="en">dcterms:Description &quot;Written, spoken, sign, and computer languages are linguistic systems.&quot;</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#AdHocCategory -->


    <owl:Class rdf:about="http://purl.org/net/CCL#AdHocCategory">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#ComplexCategory"/>
        <terms:example xml:lang="en">The categories &apos;Gym things&apos;, &apos;Approproiate Behaviour&apos; etc.</terms:example>
        <rdfs:comment xml:lang="en">Possible type of Complex Category</rdfs:comment>
        <rdfs:label xml:lang="en">Ad Hoc Category</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#Animal -->


    <owl:Class rdf:about="http://purl.org/net/CCL#Animal">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Organism"/>
        <rdfs:comment xml:lang="en">Possible type of Organism</rdfs:comment>
        <rdfs:label xml:lang="en">Animal</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#AtomicMetaphor -->


    <owl:Class rdf:about="http://purl.org/net/CCL#AtomicMetaphor">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#Metaphor"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/CCL#baseRelationContainsBelief"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Atomic Metaphor</rdfs:label>
        <skos:definition>This functions as sort of the atomic metaphor i.e. the simplest metaphors created using foundational schemas and categories.e.g. Feelings in terms of temperature, time as a moving thing</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#AtomicSchema -->


    <owl:Class rdf:about="http://purl.org/net/CCL#AtomicSchema">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#Schema"/>
        <rdfs:label xml:lang="en">Atomic Schema</rdfs:label>
        <skos:definition xml:lang="en">A basic type of collective cognition structure which encodes situations, rules, norms, behaviours etc.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#Behaviour -->


    <owl:Class rdf:about="http://purl.org/net/CCL#Behaviour">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Action"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasAssociatedCulturalAgreementStructure"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#CulturalAgreementStructure"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasContext"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#Context"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Behaviour</rdfs:label>
        <skos:definition xml:lang="en">Actions of agents within a situation/condition/circumstance</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#Category -->


    <owl:Class rdf:about="http://purl.org/net/CCL#Category">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasCategoryExemplar"/>
                                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasPrototype"/>
                                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#categoryInvokesSchema"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#Schema"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasContext"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#Context"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasCultureIdentifier"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#CultureIdentifier"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasMembershipRule"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#CategoryMembershipRule"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#CollectiveCognitionStructure"/>
        <rdfs:label xml:lang="en">Category</rdfs:label>
        <skos:definition xml:lang="en">A set of classification criteria to classify entities/situations and other structures encoded as a mental structure.

In general, categories are how one groups together various entities.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#CategoryMembershipRule -->


    <owl:Class rdf:about="http://purl.org/net/CCL#CategoryMembershipRule">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/net/CCL#Rule"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasAssociatedCategory"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#Category"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:label xml:lang="en">Category Membership Rule</rdfs:label>
        <skos:definition xml:lang="en">A rule deciding membership in some set/category</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#City -->


    <owl:Class rdf:about="http://purl.org/net/CCL#City">
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E53_Place"/>
        <rdfs:label xml:lang="en">City</rdfs:label>
        <skos:definition xml:lang="en">Usually, a geographical centre of social, political, financial activity.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#CognitiveCategory -->


    <owl:Class rdf:about="http://purl.org/net/CCL#CognitiveCategory">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#Category"/>
        <rdfs:label xml:lang="en">Atomic Category</rdfs:label>
        <skos:definition xml:lang="en">A set of classification criteria to classify entities/situations and other structures encoded as a mental structure</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#CollectiveCognitionStructure -->


    <owl:Class rdf:about="http://purl.org/net/CCL#CollectiveCognitionStructure">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#MentalObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasCultureIdentifier"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#CultureIdentifier"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Collective Cognition Structure</rdfs:label>
        <skos:definition xml:lang="en">The conceptualizations developed by a cultural community. This is a cognitive structure, arising at the cultural (group) level.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#ComplexCategory -->


    <owl:Class rdf:about="http://purl.org/net/CCL#ComplexCategory">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#Category"/>
        <rdfs:label xml:lang="en">Complex Category</rdfs:label>
        <skos:definition xml:lang="en">A set of classification criteria to classify entities/situations and other structures encoded as a mental structure</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#ComplexMetaphor -->


    <owl:Class rdf:about="http://purl.org/net/CCL#ComplexMetaphor">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#Metaphor"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasContext"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#Context"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Complex Metaphor</rdfs:label>
        <skos:definition xml:lang="en">A metaphor within the culture with distinctly cultural components such as the domain used within the expression or the expression itself such as Death as a journey to Heaven</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#ComplexSchema -->


    <owl:Class rdf:about="http://purl.org/net/CCL#ComplexSchema">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#Schema"/>
        <rdfs:label xml:lang="en">Complex Schema</rdfs:label>
        <skos:definition xml:lang="en">Collection of various schemas, categories and associated metaphors</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#Context -->


    <owl:Class rdf:about="http://purl.org/net/CCL#Context">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/CCL#containsInformationAbout"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Abstract"/>
                            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
                            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject"/>
                            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationRealization"/>
                            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object"/>
                            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Quality"/>
                            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationEntity"/>
        <rdfs:label xml:lang="en">Context</rdfs:label>
        <skos:definition xml:lang="en">An encoded frame of reference for understanding situations/behaviour/actions/language. This can be current or prior context. Prior context is context of which one has knowledge through experience or learning or simply by being exposed to it sometime in the past.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#Country -->


    <owl:Class rdf:about="http://purl.org/net/CCL#Country">
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E53_Place"/>
        <rdfs:label xml:lang="en">Country</rdfs:label>
        <skos:definition xml:lang="en">A socio-political structure encompassing a group of people</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#CulturalAgreementStructure -->


    <owl:Class rdf:about="http://purl.org/net/CCL#CulturalAgreementStructure">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#SocioCulturalStructure"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasContext"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#Context"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Cultural Agreement Structure</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#CulturalArtefact -->


    <owl:Class rdf:about="http://purl.org/net/CCL#CulturalArtefact">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#CulturalEntity"/>
        <rdfs:label xml:lang="en">Cultural Artefact</rdfs:label>
        <skos:definition xml:lang="en">A cultural artefact is an artefact which is produced and mediated by cultural agents i.e. it is a product of culture.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#CulturalBelief -->


    <owl:Class rdf:about="http://purl.org/net/CCL#CulturalBelief">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#CulturalAgreementStructure"/>
        <rdfs:label xml:lang="en">Cultural Belief</rdfs:label>
        <skos:definition xml:lang="en">A belief in relation to religion/philosophy/social rules and behaviour, for insance, held by the cultural community</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#CulturalContext -->


    <owl:Class rdf:about="http://purl.org/net/CCL#CulturalContext">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#Context"/>
        <rdfs:comment xml:lang="en">Possible type of Context</rdfs:comment>
        <rdfs:label xml:lang="en">Cultural Context</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#CulturalDomain -->


    <owl:Class rdf:about="http://purl.org/net/CCL#CulturalDomain">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#Domain"/>
        <terms:example xml:lang="en">Confucian Philosophy, Italian Renaissance, Oktoberfest</terms:example>
        <rdfs:comment xml:lang="en">Cultural domains, in the context of Cultural Linguistics, are systems of concepts specific to the culture and formed and developed uniquely within it.</rdfs:comment>
        <rdfs:label xml:lang="en">Cultural Domain</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#CulturalEntity -->


    <owl:Class rdf:about="http://purl.org/net/CCL#CulturalEntity">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#SocioCulturalStructure"/>
        <rdfs:label xml:lang="en">Cultural Entity</rdfs:label>
        <skos:definition xml:lang="en">An entity that has a cultural flavour or attribute.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#CulturalExemplar -->


    <owl:Class rdf:about="http://purl.org/net/CCL#CulturalExemplar">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/net/CCL#CulturalEntity"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#representsCulturalValue"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#CulturalValue"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#CulturalEntity"/>
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Personification"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasAssociatedCulturalValue"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#CulturalValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Cultural Exemplar</rdfs:label>
        <skos:definition xml:lang="en">An exemplar is of great significance  and is somehow distinctly representative of the culture both within and outside it. It is usually  the abstract idea of an entity usually a person displaying certain characteristics, may or may not be well-known persons. In case of an abstract idea of a person, the entity should accordingly be assigned as an abstract one.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#CulturalNorm -->


    <owl:Class rdf:about="http://purl.org/net/CCL#CulturalNorm">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/net/CCL#CulturalAgreementStructure"/>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasAssociatedBelief"/>
                                <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#CulturalBelief"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasAssociatedCulturalValue"/>
                                <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#CulturalValue"/>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasAssociatedSocialPractice"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#SocialPractice"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasContext"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#Context"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasCultureIdentifier"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#CultureIdentifier"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#CulturalAgreementStructure"/>
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Norm"/>
        <rdfs:label xml:lang="en">Cultural Norm</rdfs:label>
        <skos:definition xml:lang="en">Cultural norms are share, agreedupon beliefs and practices shared by the cultural community. 

If there is a norm which applies in all contexts, then this can be encoded as applying to a context individual whic is a high-level, general context. If associating a rule for this norm, the initial condition(s) of the asscoiated rule can simply be &apos;All conditions&apos; or &apos;Always, for example.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#CulturalSymbol -->


    <owl:Class rdf:about="http://purl.org/net/CCL#CulturalSymbol">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/net/CCL#CulturalEntity"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasSymbolism"/>
                        <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#MythologicalSymbol"/>
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SocialPerson"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasAssociatedCulturalValue"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#CulturalValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Cultural Symbol</rdfs:label>
        <skos:definition xml:lang="en">A cultural symbol could be any sort of entity that has some symbolism within the culture.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#CulturalValue -->


    <owl:Class rdf:about="http://purl.org/net/CCL#CulturalValue">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/net/CCL#CulturalAgreementStructure"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasAssociatedBelief"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#CulturalBelief"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasContext"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#Context"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasCultureIdentifier"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#CultureIdentifier"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasPriority"/>
                        <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#CulturalAgreementStructure"/>
        <rdfs:label xml:lang="en">Cultural Value</rdfs:label>
        <skos:definition xml:lang="en">Values are core ideas which stem from the beliefs underlying them and manifest within the practices of a cultural group.

The generic Description field is mandatory for this class since it will be used to record the cultural value instead of having a specific property to achieve the same thing.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#CultureIdentifier -->


    <owl:Class rdf:about="http://purl.org/net/CCL#CultureIdentifier">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasAssociatedLanguage"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#Language"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasRelevanceInTimePeriod"/>
                        <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#TimeInterval"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#isAssociatedWithPlace"/>
                        <owl:someValuesFrom rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E53_Place"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasAssociatedSocialObject"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#CollectiveAgent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Culture Identifier</rdfs:label>
        <skos:definition xml:lang="en">This class records the details of the society and culture for which the Cultural Linguistic framework is being modeled. Place is not a mandatory property since there are nomadic cultures in the world. However, the regions involved with their movement can still be coded. This is an information calss. 

The class name takes inspiration from the term &apos;File descriptor&apos; in computer Science which stores metadata describing a file like its name, size, format etc. Therefore, the culture descriptor serves as a metadata store describing a culture</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#Dialect -->


    <owl:Class rdf:about="http://purl.org/net/CCL#Dialect">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#Language"/>
        <rdfs:label xml:lang="en">Dialect</rdfs:label>
        <skos:definition xml:lang="en">A flavour of a language which may be associated with some group or subset of the cultural community. There may or may not be a region(place) associated with it.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#Domain -->


    <owl:Class rdf:about="http://purl.org/net/CCL#Domain">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SocialObject"/>
        <rdfs:label xml:lang="en">Domain</rdfs:label>
        <skos:definition xml:lang="en">A domain is a constrained subset of a larger body of knowledge. A domain can contain its own sub-domains and sub-domains are domains in their own right. When different aspects of a domain are seleted for encoding, they should be coded in the form of sub-domain individuals. 

In this ontology, a domain doesn&apos;t have a significance by itself. It may however be associated with certain contexts where applicable and can gain significance within each associated context.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#FictionalPerson -->


    <owl:Class rdf:about="http://purl.org/net/CCL#FictionalPerson">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Person"/>
        <rdfs:label xml:lang="en">Fictional Person</rdfs:label>
        <skos:definition xml:lang="en">Fictional persons, mythological figures etc. can be added in this class.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#Idea -->


    <owl:Class rdf:about="http://purl.org/net/CCL#Idea">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept"/>
        <rdfs:label xml:lang="en">Idea</rdfs:label>
        <skos:definition xml:lang="en">An idea is a concept which is either new or unseen or not completely concrete in understanding.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#Language -->


    <owl:Class rdf:about="http://purl.org/net/CCL#Language">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/net/CCL#SocioCulturalStructure"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasLinguisticSystem"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/dc/terms/LinguisticSystem"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#SocioCulturalStructure"/>
        <rdfs:label xml:lang="en">Language</rdfs:label>
        <skos:definition xml:lang="en">A system of communication and shared expression and interpretation using sounds, symbols and expressions</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#Medicine -->


    <owl:Class rdf:about="http://purl.org/net/CCL#Medicine">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#Domain"/>
        <rdfs:label xml:lang="en">Medicine</rdfs:label>
        <skos:definition xml:lang="en">Possible type of domain</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#Memory -->


    <owl:Class rdf:about="http://purl.org/net/CCL#Memory">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept"/>
        <rdfs:label xml:lang="en">Memory</rdfs:label>
        <skos:definition xml:lang="en">A knowledge representation of some past experience</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#MentalObject -->


    <owl:Class rdf:about="http://purl.org/net/CCL#MentalObject">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object"/>
        <rdfs:comment xml:lang="en">(Partly borrowed from DOLCE) - AKA &quot;internal description&quot;. Mental objects are dependent on agents which are assumed to be intentional (in the wider sense of conceiving some description).

These are classified as non-physical objects in the DOLCE(Full) Ontology.</rdfs:comment>
        <rdfs:label xml:lang="en">Mental Object</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#Metaphor -->


    <owl:Class rdf:about="http://purl.org/net/CCL#Metaphor">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/net/CCL#CollectiveCognitionStructure"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasAssociatedSchema"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#Schema"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasCultureIdentifier"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#CultureIdentifier"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasSourceDomain"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#Domain"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasTargetDomain"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#Domain"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasAnalogicalElement"/>
                        <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#CollectiveCognitionStructure"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/CCL#categoryInvokesSchema"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/net/CCL#Schema"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Metaphor</rdfs:label>
        <skos:definition xml:lang="en">A system of entity or subset of entities and/or processes which is viewed and understood within the paradigm of another.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#MythologicalSymbol -->


    <owl:Class rdf:about="http://purl.org/net/CCL#MythologicalSymbol">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#CulturalSymbol"/>
        <rdfs:comment xml:lang="en">Possible type of a Cultural Symbol</rdfs:comment>
        <rdfs:label xml:lang="en">Mythological Symbol</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#Plant -->


    <owl:Class rdf:about="http://purl.org/net/CCL#Plant">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Organism"/>
        <rdfs:comment xml:lang="en">Possible type of Organism</rdfs:comment>
        <rdfs:label xml:lang="en">Plant</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#PoliticalContext -->


    <owl:Class rdf:about="http://purl.org/net/CCL#PoliticalContext">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#Context"/>
        <rdfs:comment xml:lang="en">Possible type of Context</rdfs:comment>
        <rdfs:label xml:lang="en">Political Context</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#Politics -->


    <owl:Class rdf:about="http://purl.org/net/CCL#Politics">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#Domain"/>
        <rdfs:label xml:lang="en">Politics</rdfs:label>
        <skos:definition xml:lang="en">Possible type of domain</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#Province -->


    <owl:Class rdf:about="http://purl.org/net/CCL#Province">
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E53_Place"/>
        <rdfs:label xml:lang="en">Province</rdfs:label>
        <skos:definition xml:lang="en">Some well-dfined sub-division of a country, usually divided for administrative and political purposes</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#Region -->


    <owl:Class rdf:about="http://purl.org/net/CCL#Region">
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E53_Place"/>
        <rdfs:label xml:lang="en">Region</rdfs:label>
        <skos:definition xml:lang="en">An arbitrary grouping of physical space based on some criteria. A region,for insatnce, could include only cities of some parts of a province, some provinces or some countries.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#Rule -->


    <owl:Class rdf:about="http://purl.org/net/CCL#Rule">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasContext"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#Context"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasAssertion"/>
                        <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasCondition"/>
                        <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject"/>
        <rdfs:label xml:lang="en">Rule</rdfs:label>
        <skos:definition xml:lang="en">A combination of initial conditions which when satisfied render some target state valid</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#Schema -->


    <owl:Class rdf:about="http://purl.org/net/CCL#Schema">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://purl.org/net/CCL#encodesBehaviour"/>
                                <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#Behaviour"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://purl.org/net/CCL#encodesCategory"/>
                                <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#Category"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://purl.org/net/CCL#encodesContext"/>
                                <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#Context"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://purl.org/net/CCL#encodesCulturalAgreementStructure"/>
                                <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#CulturalAgreementStructure"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://purl.org/net/CCL#encodesMetaphor"/>
                                <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#Metaphor"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://purl.org/net/CCL#encodesRule"/>
                                <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#Rule"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://purl.org/net/CCL#encodesSocialPractice"/>
                                <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#SocialPractice"/>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasAssociatedObject"/>
                        <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasContext"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#Context"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#hasCultureIdentifier"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/net/CCL#CultureIdentifier"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.org/net/CCL#CollectiveCognitionStructure"/>
        <rdfs:label xml:lang="en">Schema</rdfs:label>
        <skos:definition xml:lang="en">Schemas are organizing structures encode knowledge about situations, events, roles, rules, expectations, goals etc.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#SocialBelief -->


