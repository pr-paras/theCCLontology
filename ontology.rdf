        <rdfs:domain rdf:resource="http://purl.org/net/CCL#Schema"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:label xml:lang="en">Has Theme</rdfs:label>
        <skos:definition xml:lang="en">Associates a schema theme (summary of the schema such as &apos;LIFE IS A JOURNEY&apos;) with the corresponding schema</skos:definition>
