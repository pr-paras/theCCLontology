        <rdfs:label xml:lang="en">Social Context</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/net/CCL#SocialPractice -->


    <owl:Class rdf:about="http://purl.org/net/CCL#SocialPractice">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/net/CCL#SocioCulturalStructure"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/net/CCL#containsEvent"/>
