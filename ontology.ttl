                                                                          rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation> ;
                                                                          rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl> ;
