                                  owl:allValuesFrom :CulturalExemplar
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasContext ;
                                  owl:allValuesFrom :Context
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasSignificanceInTimePeriod ;
                                  owl:allValuesFrom <http://www.cidoc-crm.org/cidoc-crm/E4_Period>
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasCurrentSignificance ;
                                  owl:allValuesFrom rdfs:Literal
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasCurrentSignificanceWeight ;
                                  owl:allValuesFrom xsd:float
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasDescription ;
                                  owl:allValuesFrom rdfs:Literal
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasHistoricalSignificance ;
                                  owl:allValuesFrom rdfs:Literal
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasHistoricalSignificanceWeight ;
                                  owl:allValuesFrom xsd:float
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasName ;
                                  owl:allValuesFrom rdfs:Literal
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasQuality ;
                                  owl:allValuesFrom rdfs:Literal
                                ] ;
                rdfs:label "Cultural Symbol"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> "A cultural symbol could be any sort of entity that has some symbolism within the culture."@en .


###  http://purl.org/net/CCL#CulturalValue
:CulturalValue rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                            owl:onProperty :hasAssociatedBelief ;
                                                            owl:someValuesFrom :CulturalBelief
                                                          ]
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty :hasAssociatedCulturalDescriptor ;
                                                            owl:someValuesFrom :CultureDescriptor
                                                          ]
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty :hasContext ;
                                                            owl:someValuesFrom :Context
                                                          ]
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty :hasPriority ;
                                                            owl:someValuesFrom rdfs:Literal
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ;
               rdfs:subClassOf :CulturalAgreementStructure ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :isOfValueType ;
                                 owl:allValuesFrom :ValueType
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasDescription ;
                                 owl:allValuesFrom rdfs:Literal
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :isSummarizedAs ;
                                 owl:allValuesFrom rdfs:Literal
                               ] ;
               rdfs:label "Cultural Value"@en ;
               <http://www.w3.org/2004/02/skos/core#definition> """Values are core ideas which stem from the beliefs underlying them and manifest within the practices of a cultural group.

The generic Description field is mandatory for this class since it will be used to record the cultural value instead of having a specific property to achieve the same thing."""@en .


###  http://purl.org/net/CCL#CultureDescriptor
:CultureDescriptor rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                owl:onProperty :hasAssociatedCulturalAgreementStructure ;
                                                                owl:someValuesFrom :CulturalAgreementStructure
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :hasAssociatedLanguage ;
                                                                owl:someValuesFrom :Language
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :hasRelevanceInTimePeriod ;
                                                                owl:someValuesFrom <http://www.cidoc-crm.org/cidoc-crm/E4_Period>
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                   rdfs:subClassOf :CulturalElement ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :isAssociatedWithPlace ;
                                     owl:allValuesFrom <http://www.cidoc-crm.org/cidoc-crm/E53_Place>
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasDescription ;
                                     owl:allValuesFrom rdfs:Literal
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasName ;
                                     owl:allValuesFrom rdfs:Literal
                                   ] ;
                   owl:disjointWith :HierarchyLevel ,
                                    :IntendedOutcome ,
                                    :Type ;
                   rdfs:label "Culture Descriptor"@en ;
                   <http://www.w3.org/2004/02/skos/core#definition> "This class records the details of the society and culture for which the Cultural Linguistic framework is being modeled. Place is not a mandatory property since there are nomadic cultures in the world. However, the regions involved with their movement can still be coded."@en .


###  http://purl.org/net/CCL#Dialect
:Dialect rdf:type owl:Class ;
         rdfs:subClassOf :Language ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasAssociatedEntity ;
                           owl:allValuesFrom <http://www.cidoc-crm.org/cidoc-crm/E74_Group>
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :isAssociatedWithPlace ;
                           owl:allValuesFrom <http://www.cidoc-crm.org/cidoc-crm/E53_Place>
                         ] ;
         rdfs:label "Dialect"@en ;
         <http://www.w3.org/2004/02/skos/core#definition> "A flavour of a language which may be associated with some group or subset of the cultural community. There may or may not be a region(place) associated with it."@en .


###  http://purl.org/net/CCL#Domain
:Domain rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasContext ;
                                                     owl:someValuesFrom :Context
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :isOfDomainType ;
                                                     owl:someValuesFrom :DomainType
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasDescription ;
                                                     owl:someValuesFrom rdfs:Literal
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasName ;
                                                     owl:someValuesFrom rdfs:Literal
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :hasAssociatedCulturalDescriptor ;
                          owl:allValuesFrom :CultureDescriptor
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasAssociatedEntity ;
                          owl:allValuesFrom :Entity
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasSubDomain ;
                          owl:allValuesFrom :SubDomain
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasSignificance ;
                          owl:allValuesFrom rdfs:Literal
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasSignificanceWeight ;
                          owl:allValuesFrom xsd:float
                        ] ;
        owl:disjointWith :Type ;
        rdfs:label "Domain"@en ;
        <http://www.w3.org/2004/02/skos/core#definition> """A domain is a constrained subset of a larger body of knowledge. A domain can contain its own sub-domains and sub-domains are domains in their own right. When different aspects of a domain are seleted for encoding, they should be coded in the form of sub-domain individuals. 

In this ontology, a domain doesn't have a significance by itself. It may however be associated with certain contexts where applicable and can gain significance within each associated context."""@en .


###  http://purl.org/net/CCL#DomainType
:DomainType rdf:type owl:Class ;
            rdfs:subClassOf :Type ;
            rdfs:label "Domain Type"@en ;
            <http://www.w3.org/2004/02/skos/core#definition> "A class representing a set of values which classifies domains into certain categories. The type values varies based on application and should be assigned by the application or user coding information into the ontology."@en .


###  http://purl.org/net/CCL#DomainTypeCultural
:DomainTypeCultural rdf:type owl:Class ;
                    rdfs:subClassOf :DomainType ;
                    rdfs:label "Cultural Domain Type"@en ;
                    <http://www.w3.org/2004/02/skos/core#definition> "A class representing a set of values which classifies cultural domains into certain categories. The type values varies based on application and should be assigned by the application or user coding information into the ontology."@en .


###  http://purl.org/net/CCL#Entity
:Entity rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :isOfEntityType ;
                          owl:someValuesFrom :EntityType
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasAssociatedCulturalDescriptor ;
                          owl:allValuesFrom :CultureDescriptor
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasDescription ;
                          owl:allValuesFrom rdfs:Literal
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasEntityName ;
                          owl:allValuesFrom rdfs:Literal
                        ] ;
        rdfs:label "Entity"@en ;
        <http://www.w3.org/2004/02/skos/core#definition> "An entity physical or abstract or an event which exists within reality either by form or by the recognition of its existence. For instance, a person, place, thing, organisation, event, time etc. with some concrete or abstract form of manifestation within physical reality."@en .


###  http://purl.org/net/CCL#EntityType
:EntityType rdf:type owl:Class ;
            rdfs:subClassOf :Type ;
            rdfs:label "Entity Type"@en ;
            <http://www.w3.org/2004/02/skos/core#definition> "A class representing a set of values which classifies entities into certain categories. The type values varies based on application and should be assigned by the application or user coding information into the ontology."@en .


###  http://purl.org/net/CCL#ExemplarType
:ExemplarType rdf:type owl:Class ;
              rdfs:subClassOf :CulturalEntityType ;
              rdfs:label "Exemplar Type"@en ;
              <http://www.w3.org/2004/02/skos/core#definition> "A class representing a set of values which classifies cultural exemplars into certain categories. The type values varies based on application and should be assigned by the application or user coding information into the ontology."@en .


###  http://purl.org/net/CCL#HierarchyLevel
:HierarchyLevel rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( :CategoryOrganization
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :hasAssociatedHierarchy ;
                                                             owl:someValuesFrom :CategoryHierarchy
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :hasCategoryInCurrentLevel ;
                                                             owl:someValuesFrom :Category
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :hasSuperordinateCategory ;
                                                             owl:someValuesFrom :Category
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :isAtHierarchyLevel ;
                                                             owl:someValuesFrom xsd:integer
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf :CategoryOrganization ;
                owl:disjointWith :Type ;
                rdfs:label "Hierarchy Level"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> """A classification system with categories and their subordinate categories arranged from the most general to the most specific. This will be conceptually built by inference.

This is a separate class since it is an organizational structure. Although, it may exist within the mental space, it is an abstract conceptual 'meta' structure.

This class corresponds to one level within a hierarchy. Different branches at the same level should be coded using different instances with their respective superordinate category."""@en .


###  http://purl.org/net/CCL#HierarchyType
:HierarchyType rdf:type owl:Class ;
               rdfs:subClassOf :Type ;
               rdfs:label "Hierarchy Type"@en ;
               <http://www.w3.org/2004/02/skos/core#definition> "A class representing a set of values which classifies category hierarchies into certain categories. The type values varies based on application and should be assigned by the application or user coding information into the ontology."@en .


###  http://purl.org/net/CCL#Idea
:Idea rdf:type owl:Class ;
      rdfs:subClassOf <http://www.w3.org/2004/02/skos/core#Concept> ;
      rdfs:label "Idea"@en ;
      <http://www.w3.org/2004/02/skos/core#definition> "An idea is a concept which is either new or unseen or not completely concrete in understanding."@en .


###  http://purl.org/net/CCL#IntendedOutcome
:IntendedOutcome rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                              owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                              owl:onProperty :hasAlignmentRelationshipWith ;
                                                                              owl:someValuesFrom [ rdf:type owl:Class ;
                                                                                                   owl:unionOf ( :CulturalAgreementStructure
                                                                                                                 :Rule
                                                                                                               )
                                                                                                 ]
                                                                            ]
                                                                            [ rdf:type owl:Restriction ;
                                                                              owl:onProperty :hasAlignmentObjective ;
                                                                              owl:someValuesFrom rdfs:Literal
                                                                            ]
                                                                          )
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :hasContext ;
                                                              owl:someValuesFrom :Context
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasAssociatedBehaviour ;
                                   owl:allValuesFrom :Behaviour
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasAssociatedCulturalAgreementStructure ;
                                   owl:allValuesFrom :CulturalAgreementStructure
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasAssociatedRule ;
                                   owl:allValuesFrom :Rule
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :isOfOutcomeType ;
                                   owl:allValuesFrom :OutcomeType
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasDescription ;
                                   owl:allValuesFrom rdfs:Literal
                                 ] ;
                 owl:disjointWith :Type ;
                 rdfs:label "Intended Outcome"@en ;
                 <http://www.w3.org/2004/02/skos/core#definition> "The target state/goal to be achieved by conforming to a certain rule or manipulating variables such as behaviour."@en .


###  http://purl.org/net/CCL#Language
:Language rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                       owl:onProperty :hasLinguisticSystem ;
                                                       owl:someValuesFrom <http://purl.org/dc/terms/LinguisticSystem>
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :hasName ;
                                                       owl:someValuesFrom rdfs:Literal
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf :CulturalElement ;
          owl:disjointWith :Type ;
          rdfs:label "Language"@en ;
          <http://www.w3.org/2004/02/skos/core#definition> "A system of communication and shared expression and interpretation using sounds, symbols and expressions"@en .


###  http://purl.org/net/CCL#MembershipRule
:MembershipRule rdf:type owl:Class ;
                rdfs:subClassOf :Rule ;
                rdfs:label "Membership Rule"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> "A rule deciding membership in some set"@en .


###  http://purl.org/net/CCL#Memory
:Memory rdf:type owl:Class ;
        rdfs:subClassOf :Abstract ;
        rdfs:label "Memory"@en ;
        <http://www.w3.org/2004/02/skos/core#definition> "A knowledge representation of some past experience"@en .


###  http://purl.org/net/CCL#Metaphor
:Metaphor rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :CollectiveMentalStructure
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :hasAssociatedCulturalDescriptor ;
                                                       owl:someValuesFrom :CultureDescriptor
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :hasAssociatedSchema ;
                                                       owl:someValuesFrom :Schema
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :hasSourceDomain ;
                                                       owl:someValuesFrom :Domain
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :hasTargetDomain ;
                                                       owl:someValuesFrom :Domain
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :hasAnalogicalElement ;
                                                       owl:someValuesFrom rdfs:Literal
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf :CollectiveMentalStructure ;
          rdfs:label "Metaphor"@en ;
          <http://www.w3.org/2004/02/skos/core#definition> "A system of entity or subset of entities and/or processes which is viewed and understood within the paradigm of another."@en .


###  http://purl.org/net/CCL#MetaphorType
:MetaphorType rdf:type owl:Class ;
              rdfs:subClassOf :Type ;
              rdfs:label "Metaphor Type"@en ;
              <http://www.w3.org/2004/02/skos/core#definition> "A class representing a set of values which classifies metaphors into certain categories. The type values varies based on application and should be assigned by the application or user coding information into the ontology."@en .


###  http://purl.org/net/CCL#MetaphorTypeCultural
:MetaphorTypeCultural rdf:type owl:Class ;
                      rdfs:subClassOf :MetaphorType ;
                      rdfs:label "Cultural Metaphor Type"@en ;
                      <http://www.w3.org/2004/02/skos/core#definition> "A class representing a set of values which classifies cultural metaphors into certain categories. The type values varies based on application and should be assigned by the application or user coding information into the ontology."@en .


###  http://purl.org/net/CCL#NonHumanPerson
:NonHumanPerson rdf:type owl:Class ;
                rdfs:subClassOf :Physical ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://xmlns.com/foaf/0.1/gender> ;
                                  owl:allValuesFrom rdfs:Literal
                                ] ;
                rdfs:label "Non-Human Person"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> "Animals, fictional persons, mythological figures etc. can be added in this class."@en .


###  http://purl.org/net/CCL#NormType
:NormType rdf:type owl:Class ;
          rdfs:subClassOf :Type ;
          rdfs:label "Norm Type"@en ;
          <http://www.w3.org/2004/02/skos/core#definition> "A class representing a set of values which classifies cultural norms into certain categories. The type values varies based on application and should be assigned by the application or user coding information into the ontology."@en .


###  http://purl.org/net/CCL#OutcomeType
:OutcomeType rdf:type owl:Class ;
             rdfs:subClassOf :Type ;
             rdfs:label "Outcome Type"@en ;
             <http://www.w3.org/2004/02/skos/core#definition> "A class representing a set of values which classifies intended outcomes into certain categories. The type values varies based on application and should be assigned by the application or user coding information into the ontology."@en .


###  http://purl.org/net/CCL#Physical
:Physical rdf:type owl:Class ;
          rdfs:subClassOf :Entity ;
          rdfs:label "Physical"@en ;
          <http://www.w3.org/2004/02/skos/core#definition> "Entities in the Physical wrold/reality"@en .


###  http://purl.org/net/CCL#Province
:Province rdf:type owl:Class ;
          rdfs:subClassOf :Country ;
          rdfs:label "Province"@en ;
          <http://www.w3.org/2004/02/skos/core#definition> "Some well-dfined sub-division of a country, usually divided for administrative and political purposes"@en .


###  http://purl.org/net/CCL#Region
:Region rdf:type owl:Class ;
        rdfs:subClassOf :Country ;
        rdfs:label "Region"@en ;
        <http://www.w3.org/2004/02/skos/core#definition> "An arbitrary grouping of physical space based on some criteria. A region,for insatnce, could include only cities of some parts of a province, some provinces or some countries."@en .


###  http://purl.org/net/CCL#Rule
:Rule rdf:type owl:Class ;
      owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasContext ;
                                                   owl:someValuesFrom :Context
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :isOfAssertionType ;
                                                   owl:someValuesFrom :AssertionType
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :isOfRuleType ;
                                                   owl:someValuesFrom :RuleType
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasAssertion ;
                                                   owl:someValuesFrom rdfs:Literal
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasCondition ;
                                                   owl:someValuesFrom rdfs:Literal
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
      owl:disjointWith :Type ;
      rdfs:label "Rule"@en ;
      <http://www.w3.org/2004/02/skos/core#definition> "A combination of initial conditions which when satisfied render some target state valid"@en .


###  http://purl.org/net/CCL#RuleType
:RuleType rdf:type owl:Class ;
          rdfs:subClassOf :Type ;
          rdfs:label "Rule Type"@en ;
          <http://www.w3.org/2004/02/skos/core#definition> "A class representing a set of values which classifies rules into certain categories. The type values varies based on application and should be assigned by the application or user coding information into the ontology."@en .


###  http://purl.org/net/CCL#Schema
:Schema rdf:type owl:Class ;
        rdfs:subClassOf :CollectiveMentalStructure ;
        rdfs:label "Schema"@en ;
        <http://www.w3.org/2004/02/skos/core#definition> "Schemas are organizing structures encode knowledge about situations, events, roles, rules, expectations, goals etc."@en .


###  http://purl.org/net/CCL#SchemaType
:SchemaType rdf:type owl:Class ;
            rdfs:subClassOf :Type ;
            rdfs:label "Schema Type"@en ;
            <http://www.w3.org/2004/02/skos/core#definition> "A class representing a set of values which classifies schemas into certain categories. The type values varies based on application and should be assigned by the application or user coding information into the ontology."@en .


###  http://purl.org/net/CCL#SchemaTypeCultural
:SchemaTypeCultural rdf:type owl:Class ;
                    rdfs:subClassOf :SchemaType ;
                    rdfs:label "Cultural Schema Type"@en ;
                    <http://www.w3.org/2004/02/skos/core#definition> "A class representing a set of values which classifies cultural schemas into certain categories. The type values varies based on application and should be assigned by the application or user coding information into the ontology."@en .


###  http://purl.org/net/CCL#SocialPractice
:SocialPractice rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                             owl:onProperty :containsEvent ;
                                                             owl:someValuesFrom <http://www.cidoc-crm.org/cidoc-crm/E5_Event>
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :hasAssociatedCulturalAgreementStructure ;
                                                             owl:someValuesFrom :CulturalAgreementStructure
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :hasAssociatedCulturalDescriptor ;
                                                             owl:someValuesFrom :CultureDescriptor
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :hasContext ;
                                                             owl:someValuesFrom :Context
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf :CulturalElement ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasAssociatedBehaviour ;
                                  owl:allValuesFrom :Behaviour
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasIntendedOutcome ;
                                  owl:allValuesFrom :IntendedOutcome
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasDescription ;
                                  owl:allValuesFrom rdfs:Literal
                                ] ;
                owl:disjointWith :Type ;
                rdfs:label "Social Practice"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> "A practice which even if not unique, is distinct to the culture. It has some attached cultural value to it. A social practice may be even to do 'nothing' in some given context."@en .


###  http://purl.org/net/CCL#SubDomain
:SubDomain rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( :Domain
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :isASubDomainOf ;
                                                        owl:someValuesFrom :Domain
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf :Domain ,
                           [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                    owl:onProperty :hasAssociatedCulturalAgreementStructure ;
                                                    owl:allValuesFrom :CulturalAgreementStructure
