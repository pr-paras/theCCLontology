<http://www.w3.org/ns/rdfa#prefix> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#dateTime
xsd:dateTime rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/net/CCL#categoryInvokesSchema
:categoryInvokesSchema rdf:type owl:ObjectProperty ;
                       owl:inverseOf :isASchemaUnderlying ;
                       rdfs:domain [ rdf:type owl:Class ;
                                     owl:unionOf ( :Category
                                                   :Metaphor
                                                 )
                                   ] ;
                       rdfs:range :Schema ;
                       rdfs:label "Invokes Schema"@en ;
                       <http://www.w3.org/2004/02/skos/core#definition> "The schema that a category invokes"@en .


###  http://purl.org/net/CCL#containsEvent
:containsEvent rdf:type owl:ObjectProperty ;
               owl:inverseOf :isAnEventContainedIn ;
               rdf:type owl:TransitiveProperty ;
               rdfs:domain :SocialPractice ;
               rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event> ;
               rdfs:label "Contains Event"@en ;
               <http://www.w3.org/2004/02/skos/core#definition> "Contains Some Event"@en .


###  http://purl.org/net/CCL#containsInformationAbout
:containsInformationAbout rdf:type owl:ObjectProperty ;
                          rdfs:domain :Context ;
                          rdfs:range [ rdf:type owl:Class ;
                                       owl:unionOf ( <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Abstract>
                                                     <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event>
                                                     <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject>
                                                     <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationRealization>
                                                     <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object>
                                                     <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Quality>
                                                     <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation>
                                                   )
                                     ] ;
                          rdfs:label "Contains Information About"@en ;
                          <http://www.w3.org/2004/02/skos/core#definition> "Contains Information About"@en .


###  http://purl.org/net/CCL#containsMetonym
:containsMetonym rdf:type owl:ObjectProperty ;
                 owl:inverseOf :isAMetonymUsedIn ;
                 rdfs:domain :Metaphor ;
                 rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject> ;
                 rdfs:label "Contains Metonym"@en ;
                 <http://www.w3.org/2004/02/skos/core#definition> "Metonym contained in the metaphor.Metonymy is the practice of using a concept which is a part of or closely associated with another to reference it."@en .


###  http://purl.org/net/CCL#containsSubmetaphor
:containsSubmetaphor rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :hasAssociatedMetaphor ;
                     owl:inverseOf :isASubmetaphorOf ;
                     rdf:type owl:TransitiveProperty ;
                     rdfs:domain :Metaphor ;
                     rdfs:range :Metaphor ;
                     rdfs:label "Contains Submetaphor"@en ;
                     <http://www.w3.org/2004/02/skos/core#definition> "Other metaphors contained within the this metaphor."@en .


###  http://purl.org/net/CCL#encodesBehaviour
:encodesBehaviour rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  owl:inverseOf :isABehaviourEncodedIn ;
                  rdfs:domain :Schema ;
                  rdfs:range :Behaviour ;
                  rdfs:label "Encodes Behaviour"@en ;
                  <http://www.w3.org/2004/02/skos/core#definition> "Encoded Behaviour within some schema"@en .


###  http://purl.org/net/CCL#encodesCategory
:encodesCategory rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :encodesCollectiveCognitionStructure ;
                 owl:inverseOf :isACategoryEncodedIn ;
                 rdfs:domain :Schema ;
                 rdfs:range :Category ;
                 rdfs:label "Encodes Category"@en ;
                 <http://www.w3.org/2004/02/skos/core#definition> "One or more categories encoded in the schema"@en .


###  http://purl.org/net/CCL#encodesCollectiveCognitionStructure
:encodesCollectiveCognitionStructure rdf:type owl:ObjectProperty ;
                                     owl:inverseOf :isACollectiveCognitionStructureEncodedIn ;
                                     rdfs:domain :Schema ;
                                     rdfs:range :CollectiveCognitionStructure ;
                                     rdfs:label "Encodes Collective Cognition Structure" .


###  http://purl.org/net/CCL#encodesContext
:encodesContext rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                owl:inverseOf :isAContextEncodedIn ;
                rdfs:domain :Schema ;
                rdfs:range :Context ;
                rdfs:label "Encodes Context"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> "Encoded Context within some schema"@en .


###  http://purl.org/net/CCL#encodesCulturalAgreementStructure
:encodesCulturalAgreementStructure rdf:type owl:ObjectProperty ;
                                   rdfs:subPropertyOf owl:topObjectProperty ;
                                   owl:inverseOf :isACulturalStructureEncodedIn ;
                                   rdfs:domain :Schema ;
                                   rdfs:range :CulturalAgreementStructure ;
                                   rdfs:label "Encodes Cultural Agreement Structure"@en ;
                                   <http://www.w3.org/2004/02/skos/core#definition> "Some cultural belief/norm/value encoded within a schema"@en .


###  http://purl.org/net/CCL#encodesCulturalBelief
:encodesCulturalBelief rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :encodesCulturalAgreementStructure ;
                       owl:inverseOf :isACulturalBeliefEncodedIn ;
                       rdfs:domain :Schema ;
                       rdfs:range :CulturalBelief ;
                       rdfs:label "Encodes Cultural Belief"@en ;
                       <http://www.w3.org/2004/02/skos/core#definition> "Some cultural belief encoded within a schema"@en .


###  http://purl.org/net/CCL#encodesCulturalNorm
:encodesCulturalNorm rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :encodesCulturalAgreementStructure ;
                     owl:inverseOf :isACulturalNormEncodedIn ;
                     rdfs:domain :Schema ;
                     rdfs:range :CulturalNorm ;
                     rdfs:label "Encodes Cultural Norm"@en ;
                     <http://www.w3.org/2004/02/skos/core#definition> "Some cultural norm encoded within a schema"@en .


###  http://purl.org/net/CCL#encodesCulturalValue
:encodesCulturalValue rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :encodesCulturalAgreementStructure ;
                      owl:inverseOf :isACulturalValueEncodedIn ;
                      rdfs:domain :Schema ;
                      rdfs:range :CulturalValue ;
                      rdfs:label "Encodes Cultural Value"@en ;
                      <http://www.w3.org/2004/02/skos/core#definition> "Some cultural value encoded within a schema"@en .


###  http://purl.org/net/CCL#encodesMetaphor
:encodesMetaphor rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :encodesCollectiveCognitionStructure ;
                 owl:inverseOf :isAMetaphorEncodedIn ;
                 rdfs:domain :Schema ;
                 rdfs:range :Metaphor ;
                 rdfs:label "Encodes Metaphor"@en ;
                 <http://www.w3.org/2004/02/skos/core#definition> "One or more metaphors encoded in the schema"@en .


###  http://purl.org/net/CCL#encodesRule
:encodesRule rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             owl:inverseOf :isARuleEncodedIn ;
             rdfs:domain :Schema ;
             rdfs:range :Rule ;
             rdfs:label "Encodes Rule"@en ;
             <http://www.w3.org/2004/02/skos/core#definition> "Encoded Rule within some schema"@en .


###  http://purl.org/net/CCL#encodesSchema
:encodesSchema rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :encodesCollectiveCognitionStructure ;
               owl:inverseOf :isASchemaEncodedIn ;
               rdf:type owl:TransitiveProperty ;
               rdfs:domain :ComplexSchema ;
               rdfs:range :Schema ;
               rdfs:label "Encodes Schema"@en ;
               <http://www.w3.org/2004/02/skos/core#definition> "Sub-schemas encoded in the schema"@en .


###  http://purl.org/net/CCL#encodesSocialPractice
:encodesSocialPractice rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf owl:topObjectProperty ;
                       owl:inverseOf :isASocialPracticeEncodedIn ;
                       rdfs:domain :ComplexSchema ;
                       rdfs:range :SocialPractice ;
                       rdfs:label "Encodes Social Practice"@en ;
                       <http://www.w3.org/2004/02/skos/core#definition> "Encoded Social Practice within some schema"@en .


###  http://purl.org/net/CCL#hasAlignmentRelationshipWith
:hasAlignmentRelationshipWith rdf:type owl:ObjectProperty ;
                              rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Goal> ;
                              rdfs:range [ rdf:type owl:Class ;
                                           owl:unionOf ( :CulturalAgreementStructure
                                                         :Rule
                                                       )
                                         ] ;
                              rdfs:label "Has Alignment Relationship With"@en ;
                              <http://www.w3.org/2004/02/skos/core#definition> "Assigns alignment objective of a  concept with another which it wants to attain alignment objective - Rule/Norm/Value/Belief"@en .


###  http://purl.org/net/CCL#hasAssociatedAction
:hasAssociatedAction rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :hasAssociatedEvent ;
                     owl:inverseOf :isAnActionAssociatedWith ;
                     rdfs:domain :Behaviour ;
                     rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Action> ;
                     rdfs:label "Has Associated Action"@en ;
                     <http://www.w3.org/2004/02/skos/core#definition> "One or more associated actions"@en .


###  http://purl.org/net/CCL#hasAssociatedArtefact
:hasAssociatedArtefact rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :hasAssociatedSocialObject ;
                       owl:inverseOf :isAnAssociatedArtefactOf ;
                       rdfs:range :CulturalArtefact ;
                       rdfs:label "Has Associated Artefact"@en ;
                       <http://www.w3.org/2004/02/skos/core#definition> "Some Associated Artefact"@en .


###  http://purl.org/net/CCL#hasAssociatedAtomicCategory
:hasAssociatedAtomicCategory rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf :hasAssociatedCategory ;
                             owl:inverseOf :isAnAssociatedAtomicCategoryOf ;
                             rdfs:domain [ rdf:type owl:Class ;
                                           owl:unionOf ( :CategoryMembershipRule
                                                         :CollectiveCognitionStructure
                                                       )
                                         ] ;
                             rdfs:range :CognitiveCategory ;
                             rdfs:label "Has Associated Atomic Category"@en ;
                             <http://www.w3.org/2004/02/skos/core#definition> "Assigns associated atomic categories"@en .


###  http://purl.org/net/CCL#hasAssociatedAtomicMetaphor
:hasAssociatedAtomicMetaphor rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf :hasAssociatedMetaphor ;
                             owl:inverseOf :isAnAssociatedAtomicMetaphorOf ;
                             rdfs:domain :Metaphor ;
                             rdfs:range :AtomicMetaphor ;
                             rdfs:label "Has Associated Atomic Metaphor"@en ;
                             <http://www.w3.org/2004/02/skos/core#definition> "Assigns an associated atomic metaphor"@en .


###  http://purl.org/net/CCL#hasAssociatedAtomicSchema
:hasAssociatedAtomicSchema rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf :hasAssociatedSchema ;
                           owl:inverseOf :isAnAssociatedAtomicSchemaOf ;
                           rdfs:domain :CollectiveCognitionStructure ;
                           rdfs:range :AtomicSchema ;
                           rdfs:label "Has Associated Atomic Schema"@en ;
                           <http://www.w3.org/2004/02/skos/core#definition> "Assigns associated atomic schemas"@en .


###  http://purl.org/net/CCL#hasAssociatedBehaviour
:hasAssociatedBehaviour rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :hasAssociatedAction ;
                        owl:inverseOf :isABehaviourAssociatedWith ;
                        rdfs:domain [ rdf:type owl:Class ;
                                      owl:unionOf ( :CulturalNorm
                                                    :SocialPractice
                                                    <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Goal>
                                                  )
                                    ] ;
                        rdfs:range :Behaviour ;
                        rdfs:label "Has Associated Behaviour"@en ;
                        <http://www.w3.org/2004/02/skos/core#definition> "One or more associated behaviours especially for rule and behaviour schemas"@en .


###  http://purl.org/net/CCL#hasAssociatedBelief
:hasAssociatedBelief rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :hasAssociatedCulturalAgreementStructure ;
                     owl:inverseOf :isACulturalBeliefAssociatedWith ;
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( :CulturalNorm
                                                 :CulturalValue
                                               )
                                 ] ;
                     rdfs:range :CulturalBelief ;
                     rdfs:label "Has Associated Cultural Belief"@en ;
                     <http://www.w3.org/2004/02/skos/core#definition> "One or more associated cultural beliefs"@en .


###  http://purl.org/net/CCL#hasAssociatedCategory
:hasAssociatedCategory rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :hasAssociatedCollectiveCognitionStructure ;
                       owl:inverseOf :isAnAssociatedCategoryOf ;
                       rdfs:domain [ rdf:type owl:Class ;
                                     owl:unionOf ( :CategoryMembershipRule
                                                   :CollectiveCognitionStructure
                                                   :CulturalEntity
                                                 )
                                   ] ;
                       rdfs:range :Category ;
                       rdfs:label "Has Associated Category"@en ;
                       <http://www.w3.org/2004/02/skos/core#definition> "Categories subsumed in a category hierarchy"@en ,
                                                                        "One or more associated categories"@en .


###  http://purl.org/net/CCL#hasAssociatedCollectiveCognitionStructure
:hasAssociatedCollectiveCognitionStructure rdf:type owl:ObjectProperty ;
                                           rdfs:subPropertyOf :hasAssociatedObject ;
                                           owl:inverseOf :isACollectiveCognitionStructureAssociatedWith ;
                                           rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity> ;
                                           rdfs:range :CollectiveCognitionStructure ;
                                           rdfs:label "Has Associated Collective Cognition Structure"@en ;
                                           <http://www.w3.org/2004/02/skos/core#definition> "One or more associated schemas,categories or metaphors"@en .


###  http://purl.org/net/CCL#hasAssociatedComplexCategory
:hasAssociatedComplexCategory rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf :hasAssociatedCategory ;
                              owl:inverseOf :isAnAssociatedComplexCategoryOf ;
                              rdfs:domain [ rdf:type owl:Class ;
                                            owl:unionOf ( :CategoryMembershipRule
                                                          :CollectiveCognitionStructure
                                                          :CulturalEntity
                                                        )
                                          ] ;
                              rdfs:range :ComplexCategory ;
                              rdfs:label "Has Associated Complex Category"@en ;
                              <http://www.w3.org/2004/02/skos/core#definition> "Assigns associated complex categories"@en .


###  http://purl.org/net/CCL#hasAssociatedComplexMetaphor
:hasAssociatedComplexMetaphor rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf :hasAssociatedMetaphor ;
                              owl:inverseOf :isAnAssociatedComplexMetaphorOf ;
                              rdfs:domain :Metaphor ;
                              rdfs:range :ComplexMetaphor ;
                              rdfs:label "Has Associated Complex Metaphor"@en ;
                              <http://www.w3.org/2004/02/skos/core#definition> "Assigns associated complex metaphors"@en .


###  http://purl.org/net/CCL#hasAssociatedComplexSchema
:hasAssociatedComplexSchema rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf :hasAssociatedSchema ;
                            owl:inverseOf :isAnAssociatedComplexSchemaOf ;
                            rdfs:domain :CollectiveCognitionStructure ;
                            rdfs:range :ComplexSchema ;
                            rdfs:label "Has Associated Complex Schema"@en ;
                            <http://www.w3.org/2004/02/skos/core#definition> "Assigns associated complex schemas"@en .


###  http://purl.org/net/CCL#hasAssociatedCulturalAgreementStructure
:hasAssociatedCulturalAgreementStructure rdf:type owl:ObjectProperty ;
                                         rdfs:subPropertyOf :hasAssociatedSocialObject ;
                                         owl:inverseOf :isAnAssociatedCulturalStructureOf ;
                                         rdfs:domain [ rdf:type owl:Class ;
                                                       owl:unionOf ( :Behaviour
                                                                     :ComplexMetaphor
                                                                     :SocialPractice
                                                                     <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Goal>
                                                                   )
                                                     ] ;
                                         rdfs:range :CulturalAgreementStructure ;
                                         rdfs:label "Has Associated Cultural Agreement Structure"@en ;
                                         <http://www.w3.org/2004/02/skos/core#definition> "Has some cultural belief/value/norm"@en ,
                                                                                          "The underlying belief/value or other cultural structure which led to a particular target being mapped to a particular source in a metaphor"@en .


###  http://purl.org/net/CCL#hasAssociatedCulturalNorm
:hasAssociatedCulturalNorm rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf :hasAssociatedCulturalAgreementStructure ;
                           owl:inverseOf :isACulturalNormAssociatedWith ;
                           rdfs:domain :SocialPractice ;
                           rdfs:range :CulturalNorm ;
                           rdfs:label "Has Associated Cultural Norm"@en ;
                           <http://www.w3.org/2004/02/skos/core#definition> "One or more associated norms."@en .


###  http://purl.org/net/CCL#hasAssociatedCulturalValue
:hasAssociatedCulturalValue rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf :hasAssociatedCulturalAgreementStructure ;
                            owl:inverseOf :isACulturalValueAssociatedWith ;
                            rdfs:domain [ rdf:type owl:Class ;
                                          owl:unionOf ( :CulturalBelief
                                                        :CulturalEntity
                                                        :CulturalNorm
                                                      )
                                        ] ;
                            rdfs:range :CulturalValue ;
                            rdfs:label "Has Associated Cultural Value"@en ;
                            <http://www.w3.org/2004/02/skos/core#definition> "One or more cultural values associated with the concept."@en .


###  http://purl.org/net/CCL#hasAssociatedDomain
:hasAssociatedDomain rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :hasAssociatedSocialObject ;
                     owl:inverseOf :isADomainOf ;
                     rdf:type owl:IrreflexiveProperty ;
                     rdfs:range :Domain ;
                     rdfs:label "Has Associated Domain"@en ;
                     <http://www.w3.org/2004/02/skos/core#definition> "Associates a domain as a domain of a metaphor"@en .


###  http://purl.org/net/CCL#hasAssociatedEvent
:hasAssociatedEvent rdf:type owl:ObjectProperty ;
                    owl:inverseOf :isAnEventAssociatedWith ;
                    rdfs:domain :Domain ;
                    rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event> ;
                    rdfs:label "Has Associated Event"@en ;
                    <http://www.w3.org/2004/02/skos/core#definition> "Assigns one or more associated events"@en .


###  http://purl.org/net/CCL#hasAssociatedExemplar
:hasAssociatedExemplar rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :hasAssociatedSocialObject ;
                       owl:inverseOf :isAnAssociatedExemplarOf ;
                       rdfs:domain :CulturalSymbol ;
                       rdfs:range :CulturalExemplar ;
                       rdfs:label "Has Associated Exemplar"@en ;
                       <http://www.w3.org/2004/02/skos/core#definition> "One or more associated cultural exemplars"@en .


###  http://purl.org/net/CCL#hasAssociatedLanguage
:hasAssociatedLanguage rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :hasAssociatedSocialObject ;
                       owl:inverseOf :isAnAssociatedLanguageOf ;
                       rdfs:domain :CultureIdentifier ;
                       rdfs:range :Language ;
                       rdfs:label "Has Associated Language"@en ;
                       <http://www.w3.org/2004/02/skos/core#definition> """Language associated with the culture.

This is not a transitive property as it is possible that the same language is associated with different cultures. For instance, English."""@en .


###  http://purl.org/net/CCL#hasAssociatedMetaphor
:hasAssociatedMetaphor rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :hasAssociatedCollectiveCognitionStructure ;
                       owl:inverseOf :isAnAssociatedMetaphorOf ;
                       rdfs:domain :Metaphor ;
                       rdfs:range :Metaphor ;
                       rdfs:label "Has Associated Metaphor"@en ;
                       <http://www.w3.org/2004/02/skos/core#definition> "One or more associated metaphors"@en .


###  http://purl.org/net/CCL#hasAssociatedObject
:hasAssociatedObject rdf:type owl:ObjectProperty ;
                     rdfs:comment "Assigns some associated object"@en ;
                     rdfs:label "Has Associated Object" .


###  http://purl.org/net/CCL#hasAssociatedPhysicalObject
:hasAssociatedPhysicalObject rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf :hasAssociatedObject ,
                                                :isAnAssociatedObjectOf ;
                             owl:inverseOf :isAnAssociatedPhysicalObjectOf ;
