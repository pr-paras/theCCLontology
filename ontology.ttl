###  http://purl.org/dc/terms/LinguisticSystem
<http://purl.org/dc/terms/LinguisticSystem> rdf:type owl:Class ;
                                            rdfs:subClassOf :CulturalElement ,
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :hasDescription ;
                                                              owl:someValuesFrom rdfs:Literal
                                                            ] ,
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :hasName ;
                                                              owl:allValuesFrom rdfs:Literal
                                                            ] ;
                                            rdfs:label "Linguistic System"@en ;
                                            <http://www.w3.org/2004/02/skos/core#definition> "dcterms:Description \"Written, spoken, sign, and computer languages are linguistic systems.\""@en .


###  http://purl.org/net/CCL#Abstract
:Abstract rdf:type owl:Class ;
          rdfs:subClassOf :Entity ;
          rdfs:label "Abstract"@en .


###  http://purl.org/net/CCL#Action
:Action rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasContext ;
                                                     owl:someValuesFrom :Context
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :isOfActionType ;
                                                     owl:someValuesFrom :ActionType
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :hasActionQuality ;
                          owl:allValuesFrom rdfs:Literal
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasDescription ;
                          owl:allValuesFrom rdfs:Literal
                        ] ;
        owl:disjointWith :CategoryOrganization ,
                         :CollectiveMentalStructure ,
                         :Physical ,
                         :Type ;
        rdfs:label "Action"@en ;
        <http://www.w3.org/2004/02/skos/core#definition> "Movement(Specifically travel over a distance)/Speech/Silence/Non-action/Facial or other Gesture/Emotion expression"@en .


###  http://purl.org/net/CCL#ActionType
:ActionType rdf:type owl:Class ;
            rdfs:subClassOf :Type ;
            rdfs:label "Action Type"@en ;
            <http://www.w3.org/2004/02/skos/core#definition> "A class representing a set of values which classifies actions into certain categories. The type values varies based on application and should be assigned by the application or user coding information into the ontology."@en .


###  http://purl.org/net/CCL#ArtefactType
:ArtefactType rdf:type owl:Class ;
              rdfs:subClassOf :CulturalEntityType ;
              rdfs:label "Artefact Type"@en ;
              <http://www.w3.org/2004/02/skos/core#definition> "A class representing a set of values which classifies cultural artefacts into certain categories. The type values varies based on application and should be assigned by the application or user coding information into the ontology."@en .


###  http://purl.org/net/CCL#AssertionType
:AssertionType rdf:type owl:Class ;
               rdfs:subClassOf :Type ;
               rdfs:label "Assertion Type"@en ;
               <http://www.w3.org/2004/02/skos/core#definition> "A class representing a set of values which classifies rule assertions into certain categories. The type values varies based on application and should be assigned by the application or user coding information into the ontology."@en .


###  http://purl.org/net/CCL#Behaviour
:Behaviour rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                        owl:onProperty :hasAssociatedAction ;
                                                        owl:someValuesFrom :Action
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :hasAssociatedCulturalAgreementStructure ;
                                                        owl:someValuesFrom :CulturalAgreementStructure
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :hasContext ;
                                                        owl:someValuesFrom :Context
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :hasDescription ;
                                                        owl:someValuesFrom rdfs:Literal
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :hasAssociatedRule ;
                             owl:allValuesFrom :Rule
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasIntendedOutcome ;
                             owl:allValuesFrom :IntendedOutcome
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasBehaviourQuality ;
                             owl:allValuesFrom rdfs:Literal
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasExpectedComponent ;
                             owl:allValuesFrom rdfs:Literal
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasPerceivedComponent ;
                             owl:allValuesFrom rdfs:Literal
                           ] ;
           owl:disjointWith :CategoryOrganization ,
                            :CultureDescriptor ,
                            :HierarchyLevel ,
                            :Physical ,
                            :Type ;
           rdfs:label "Behaviour"@en ;
           <http://www.w3.org/2004/02/skos/core#definition> "Actions of agents within a situation/condition/circumstance"@en .


###  http://purl.org/net/CCL#BeliefType
:BeliefType rdf:type owl:Class ;
            rdfs:subClassOf :Type ;
            rdfs:label "Belief Type"@en ;
            <http://www.w3.org/2004/02/skos/core#definition> "A class representing a set of values which classifies cultural beliefs into certain categories. The type values varies based on application and should be assigned by the application or user coding information into the ontology."@en .


###  http://purl.org/net/CCL#Category
:Category rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                       owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                       owl:onProperty :hasCategoryExemplar ;
                                                                       owl:someValuesFrom :Entity
                                                                     ]
                                                                     [ rdf:type owl:Restriction ;
                                                                       owl:onProperty :hasPrototype ;
                                                                       owl:someValuesFrom :Entity
                                                                     ]
                                                                   )
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :categoryInvokesSchema ;
                                                       owl:someValuesFrom :Schema
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :hasAssociatedCulturalDescriptor ;
                                                       owl:someValuesFrom :CultureDescriptor
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :hasContext ;
                                                       owl:someValuesFrom :Context
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :hasMembershipRule ;
                                                       owl:someValuesFrom :CategoryMembershipRule
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf :CollectiveMentalStructure ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasAssociatedEntity ;
                            owl:allValuesFrom :Entity
                          ] ;
          rdfs:label "Category"@en ;
          <http://www.w3.org/2004/02/skos/core#definition> """A set of classification criteria to classify entities/situations and other structures encoded as a mental structure.

In general, categories are how one groups together various entities."""@en .


###  http://purl.org/net/CCL#CategoryHierarchy
:CategoryHierarchy rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( :CategoryOrganization
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :hasRootCategory ;
                                                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                owl:onClass :Category
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :hasTotalLevelsInHierarchy ;
                                                                owl:someValuesFrom xsd:integer
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                   rdfs:subClassOf :CategoryOrganization ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasContext ;
                                     owl:allValuesFrom :Context
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :isOfHierarchyType ;
                                     owl:allValuesFrom :HierarchyType
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasDescription ;
                                     owl:allValuesFrom rdfs:Literal
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasName ;
                                     owl:allValuesFrom rdfs:Literal
                                   ] ;
                   rdfs:label "Category Hierarchy"@en ;
                   <http://www.w3.org/2004/02/skos/core#definition> """A classification system with categories and their subordinate categories arranged from the most general to the most specific. This will be conceptually built by inference.

This is a separate class since it is an organizational structure. Although, it may exist within the mental space, it is an abstract conceptual 'meta' structure.

This class serves to identify a hierarchy."""@en .


###  http://purl.org/net/CCL#CategoryMembershipRule
:CategoryMembershipRule rdf:type owl:Class ;
                        owl:equivalentClass [ owl:intersectionOf ( :MembershipRule
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty :hasAssociatedCategory ;
                                                                     owl:someValuesFrom :Category
                                                                   ]
                                                                 ) ;
                                              rdf:type owl:Class
                                            ] ;
                        rdfs:subClassOf :MembershipRule ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :membershipRuleHasWeightage ;
                                          owl:allValuesFrom xsd:float
                                        ] ;
                        rdfs:label "Category Membership Rule"@en ;
                        <http://www.w3.org/2004/02/skos/core#definition> "A membership rule that is forms a part of some category"@en .


###  http://purl.org/net/CCL#CategoryOrganization
:CategoryOrganization rdf:type owl:Class ;
                      owl:disjointWith :IntendedOutcome ,
                                       :Type ;
                      rdfs:label "Category Organization"@en ;
                      <http://www.w3.org/2004/02/skos/core#definition> "A class denoting some organization of categories, either in a hierarchy or in the same level."@en .


###  http://purl.org/net/CCL#CategoryType
:CategoryType rdf:type owl:Class ;
              rdfs:subClassOf :Type ;
              rdfs:label "Category Type"@en ;
              <http://www.w3.org/2004/02/skos/core#definition> "A class representing a set of values which classifies categories into certain categories. The type values varies based on application and should be assigned by the application or user coding information into the ontology."@en .


###  http://purl.org/net/CCL#CategoryTypeCultural
:CategoryTypeCultural rdf:type owl:Class ;
                      rdfs:subClassOf :CategoryType ;
                      rdfs:label "Cultural Category Type"@en ;
                      <http://www.w3.org/2004/02/skos/core#definition> "A class representing a set of values which classifies cultural categories into certain categories. The type values varies based on application and should be assigned by the application or user coding information into the ontology."@en .


###  http://purl.org/net/CCL#City
:City rdf:type owl:Class ;
      rdfs:subClassOf :Country ;
      rdfs:label "City"@en ;
      <http://www.w3.org/2004/02/skos/core#definition> "Usually, a geographical centre of social, political, financial activity."@en .


###  http://purl.org/net/CCL#CognitiveCategory
:CognitiveCategory rdf:type owl:Class ;
                   rdfs:subClassOf :Category ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasAssociatedCategory ;
                                     owl:allValuesFrom :Category
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasAssociatedHierarchy ;
                                     owl:allValuesFrom :CategoryHierarchy
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasAssociatedSchema ;
                                     owl:allValuesFrom :Schema
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasSubCategory ;
                                     owl:allValuesFrom :Category
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :isACategoryEncodedIn ;
                                     owl:allValuesFrom :Schema
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :isOfCategoryType ;
                                     owl:allValuesFrom :CategoryType
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasAssociatedLinguisticExpression ;
                                     owl:allValuesFrom rdfs:Literal
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasDescription ;
                                     owl:allValuesFrom rdfs:Literal
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasName ;
                                     owl:allValuesFrom rdfs:Literal
                                   ] ;
                   rdfs:label "Cognitive Category"@en ;
                   <http://www.w3.org/2004/02/skos/core#definition> "A set of classification criteria to classify entities/situations and other structures encoded as a mental structure"@en .


###  http://purl.org/net/CCL#CognitiveMetaphor
:CognitiveMetaphor rdf:type owl:Class ;
                   rdfs:subClassOf :Metaphor ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :containsMetonym ;
                                     owl:allValuesFrom :Entity
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :containsSubmetaphor ;
                                     owl:allValuesFrom :Metaphor
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasAssociatedCategory ;
                                     owl:allValuesFrom :Category
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasAssociatedEntity ;
                                     owl:allValuesFrom :Entity
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasAssociatedMetaphor ;
                                     owl:allValuesFrom :Metaphor
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasContext ;
                                     owl:allValuesFrom :Context
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasPreConceptualization ;
                                     owl:allValuesFrom :CollectiveMentalStructure
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :isAMetaphorEncodedIn ;
                                     owl:allValuesFrom :Schema
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :makesReferenceToEntity ;
                                     owl:allValuesFrom :Entity
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :baseRelationContainsBelief ;
                                     owl:someValuesFrom rdfs:Literal
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasGeneralConnotation ;
                                     owl:allValuesFrom rdfs:Literal
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasIntendedMeaning ;
                                     owl:allValuesFrom rdfs:Literal
                                   ] ;
                   rdfs:label "Cognitive Metaphor"@en ;
                   <http://www.w3.org/2004/02/skos/core#definition> "This functions as sort of the atomic metaphor i.e. the simplest metaphors created using foundational schemas and categories.e.g. Feelings in terms of temperature." .


###  http://purl.org/net/CCL#CognitiveSchema
:CognitiveSchema rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                              owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                              owl:onProperty :encodesBehaviour ;
                                                                              owl:someValuesFrom :Behaviour
                                                                            ]
                                                                            [ rdf:type owl:Restriction ;
                                                                              owl:onProperty :encodesCategory ;
                                                                              owl:someValuesFrom :CognitiveCategory
                                                                            ]
                                                                            [ rdf:type owl:Restriction ;
                                                                              owl:onProperty :encodesContext ;
                                                                              owl:someValuesFrom :Context
                                                                            ]
                                                                            [ rdf:type owl:Restriction ;
                                                                              owl:onProperty :encodesMetaphor ;
                                                                              owl:someValuesFrom :CognitiveMetaphor
                                                                            ]
                                                                            [ rdf:type owl:Restriction ;
                                                                              owl:onProperty :encodesRule ;
                                                                              owl:someValuesFrom :Rule
                                                                            ]
                                                                          )
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :hasAssociatedCulturalDescriptor ;
                                                              owl:someValuesFrom :CultureDescriptor
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :hasAssociatedEntity ;
                                                              owl:someValuesFrom :Entity
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :hasContext ;
                                                              owl:someValuesFrom :Context
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :isOfSchemaType ;
                                                              owl:someValuesFrom :SchemaType
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf :Schema ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :encodesSchema ;
                                   owl:allValuesFrom :CognitiveSchema
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasAssociatedSchema ;
                                   owl:allValuesFrom :Schema
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :containsIdeaInSchema ;
                                   owl:allValuesFrom rdfs:Literal
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :containsIdeaInTheme ;
                                   owl:allValuesFrom rdfs:Literal
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasAssociatedLinguisticExpression ;
                                   owl:allValuesFrom rdfs:Literal
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasDescription ;
                                   owl:allValuesFrom rdfs:Literal
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasName ;
                                   owl:allValuesFrom rdfs:Literal
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasRelationshipBetweenThemeEntities ;
                                   owl:allValuesFrom rdfs:Literal
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasTheme ;
                                   owl:allValuesFrom rdfs:Literal
