#################################################################
#    Object Properties
#################################################################

###  http://purl.org/net/CCL#categoryInvokesSchema
:categoryInvokesSchema rdf:type owl:ObjectProperty ;
                       owl:inverseOf :isASchemaUnderlyingCategory ;
                       rdfs:domain :Category ;
                       rdfs:range :Schema ;
                       rdfs:label "Category Invokes Schema"@en ;
                       <http://www.w3.org/2004/02/skos/core#definition> "The schema that a category invokes"@en .


###  http://purl.org/net/CCL#containsEvent
:containsEvent rdf:type owl:ObjectProperty ;
               owl:inverseOf :isAnEventContainedIn ;
               rdf:type owl:TransitiveProperty ;
               rdfs:range <http://www.cidoc-crm.org/cidoc-crm/E5_Event> ;
               rdfs:label "Contains Event"@en ;
               <http://www.w3.org/2004/02/skos/core#definition> "Contains Some Event"@en .


###  http://purl.org/net/CCL#containsInformationAbout
:containsInformationAbout rdf:type owl:ObjectProperty ;
                          rdfs:domain :Context ;
                          rdfs:label "Contains Information About"@en ;
                          <http://www.w3.org/2004/02/skos/core#definition> "Contains Information About"@en .


###  http://purl.org/net/CCL#containsMetonym
:containsMetonym rdf:type owl:ObjectProperty ;
                 owl:inverseOf :isAMetonymUsedIn ;
                 rdfs:label "Contains Metonym"@en ;
                 <http://www.w3.org/2004/02/skos/core#definition> "Metonym contained in the metaphor.Metonymy is the practice of using a concept which is a part of or closely associated with another to reference it."@en .


###  http://purl.org/net/CCL#containsSubmetaphor
:containsSubmetaphor rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :hasAssociatedMentalStructure ;
                     owl:inverseOf :isASubmetaphorOf ;
                     rdf:type owl:TransitiveProperty ;
                     rdfs:domain :Metaphor ;
                     rdfs:range :Metaphor ;
                     rdfs:label "Contains Submetaphor"@en ;
                     <http://www.w3.org/2004/02/skos/core#definition> "Other metaphors contained within the this metaphor."@en .


###  http://purl.org/net/CCL#encodesBehaviour
:encodesBehaviour rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  owl:inverseOf :isABehaviourEncodedIn ;
                  rdfs:domain :Schema ;
                  rdfs:range :Behaviour ;
                  rdfs:label "Encodes Behaviour"@en ;
                  <http://www.w3.org/2004/02/skos/core#definition> "Encoded Behaviour within some schema"@en .


###  http://purl.org/net/CCL#encodesCategory
:encodesCategory rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :hasAssociatedMentalStructure ;
                 owl:inverseOf :isACategoryEncodedIn ;
                 rdfs:domain :Schema ;
                 rdfs:range :Category ;
                 rdfs:label "Encodes Category"@en ;
                 <http://www.w3.org/2004/02/skos/core#definition> "One or more categories encoded in the schema"@en .


###  http://purl.org/net/CCL#encodesContext
:encodesContext rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                owl:inverseOf :isAContextEncodedIn ;
                rdfs:domain :Schema ;
                rdfs:range :Context ;
                rdfs:label "Encodes Context"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> "Encoded Context within some schema"@en .


###  http://purl.org/net/CCL#encodesCulturalAgreementStructure
